{
  "name": "asp-mypage",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --dotenv .env.development",
    "build_dev": "nuxt build --dotenv .env.development",
    "build_staging": "nuxt build --dotenv .env.staging",
    "build_production": "nuxt build --dotenv .env.production",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "bootstrap": "^5.2.3",
    "clipboard": "^2.0.11",
    "core-js": "^3.25.3",
    "js-cookie": "^3.0.1",
    "nuxt": "^2.15.8",
    "nuxt-lazy-load": "^1.2.9",
    "swiper": "^8.4.5",
    "vue": "^2.7.10",
    "vue-i18n": "^8.28.2",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.1",
    "eslint-plugin-vue": "^9.5.1",
    "prettier": "^2.7.1"
  }
}
